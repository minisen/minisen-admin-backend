@model List<Account_ShowDTO>
{
    "status": "success",
    "timestamp": "@(DateTime.Now.ToString())",
    "sendData": {
        "ItemsPaged": [
            @foreach (var item in Model)
            {
                <text>
                    {
                        "id": "@(item.Id)",
                        "name": "@(item.Name)",
                        "account": "@(item.Account)",
                        "roles": "@(item.Roles)",
                        "remarks": "@(item.RemarkInfo)",
                        "hasRoleIds": [
                            @Html.Raw(0 == item.HasRoleIds.Count() ? "" : $"\"{string.Join(@""",""", item.HasRoleIds)}\"")
                        ]
                    }@((Model.IndexOf(item) != Model.Count - 1) ? "," : "")
                </text>
            }

